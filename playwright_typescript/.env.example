# Example .env file for Playwright TypeScript
# Copy this file to .env and update with your credentials

# Bagisto Store URLs
BAGISTO_BASE_URL=https://commerce.bagisto.com
BAGISTO_ADMIN_URL=https://commerce.bagisto.com/admin

# Admin Credentials (Bagisto Demo - auto-filled)
BAGISTO_ADMIN_EMAIL=admin@example.com
BAGISTO_ADMIN_PASSWORD=admin123

# User Credentials (Bagisto Demo - auto-login)
BAGISTO_USER_EMAIL=john@example.com
BAGISTO_USER_PASSWORD=password123

# Test Configuration
HEADLESS=false  # Set to 'true' for headless browser mode
